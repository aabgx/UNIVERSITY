    13                                      ; d/[(a+b)-(c+c)]   a,b,c - byte        d - word
    14 00000000 03                          a db 3
    15 00000001 04                          b db 4
    16 00000002 02                          c db 2
    17 00000003 0C00                        d dw 12
    18                                      ; -> 4
    19                                  ; our code starts here
    20                                  segment code use32 class=code
    21                                      start:
    22 00000000 8A1D[00000000]                  mov bl,[a]
    23 00000006 021D[01000000]                  add bl,[b]
    24 0000000C 2A1D[02000000]                  sub bl,[c]
    25 00000012 2A1D[02000000]                  sub bl,[c] ; al = (a+b)-(c+c)
    26 00000018 66A1[03000000]                  mov ax,[d]
